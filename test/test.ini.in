# test configuration

[global]
a2md_bin = ../src/a2md
a2md_version = @PACKAGE_VERSION@
prefix = @PREFIX@
server_name = @SERVER_NAME@
server_dir = @SERVER_DIR@
http_port = @HTTP_PORT@
https_port = @HTTPS_PORT@
http_proxy_port = @HTTP_PROXY_PORT@
gen_dir = @GEN_DIR@
curl_bin = @CURL_BIN@
openssl_bin = @OPENSSL_BIN@

[acme]
url_default=@ACME_DEF_URL@
url = @ACME_TEST_URL@
tos = accepted
server= @ACME_TEST_TYPE@