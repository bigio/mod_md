[global]
curl_bin = @CURL_BIN@
openssl_bin = @OPENSSL_BIN@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
sbindir = @sbindir@
libdir = @libdir@
libexecdir = @libexecdir@

apxs = @bindir@/apxs
apachectl = @sbindir@/apachectl

a2md_bin = @bindir@/a2md
a2md_version = @PACKAGE_VERSION@

[httpd]
version = @HTTPD_VERSION@
name = @progname@
user = @HTTPD_USER@

[test]
gen_dir = @abs_srcdir@/gen
test_dir = @abs_srcdir@
test_src_dir = @abs_srcdir@
http_port = @HTTP_PORT@
https_port = @HTTPS_PORT@
proxy_port = @HTTP_PROXY_PORT@
http_tld = tests.httpd.apache.org

[acme]
url_default=@ACME_DEF_URL@
url = @ACME_TEST_URL@
tos = accepted
server= @ACME_TEST_TYPE@
server_dir= @ACME_TEST_DIR@